{
  "name": "ts-transform-react-jsx-source",
  "version": "2.0.3",
  "description": "Adds source file and line number to JSX elements.",
  "main": "dist/index.js",
  "scripts": {
    "test": "rm -rf test/fixture/*.js && tsc && jest",
    "prettier": "prettier --write '{src,test}/**/*.ts*' || true",
    "prepublishOnly": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dropbox/ts-transform-react-jsx-source.git"
  },
  "keywords": [
    "typescript",
    "transform",
    "ts",
    "react",
    "jsx",
    "source"
  ],
  "author": "Long Ho <longho@dropbox.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dropbox/ts-transform-react-jsx-source/issues"
  },
  "homepage": "https://github.com/dropbox/ts-transform-react-jsx-source#readme",
  "dependencies": {
    "typescript": "4"
  },
  "peerDependencies": {
    "ts-jest": "^26.4.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.4",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^18.7.6",
    "@types/react-dom": "^17.0.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "husky": "5",
    "jest": "^26.6.3",
    "mocha": "^8.2.1",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.jest.json",
        "astTransformers": {
          "before": [
            "./dist/ts-jest-integration.js"
          ]
        }
      }
    }
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "parser": "typescript"
  }
}
